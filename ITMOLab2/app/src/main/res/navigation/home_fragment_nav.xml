<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home_fragment_nav"
    app:startDestination="@+id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.itmolab2.home.HomeFragment"
        android:label="homeFragment">
        <action
            android:id="@+id/action_homeFragment_to_homeOpenedFragment"
            app:destination="@id/homeOpenedFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeOpenedFragment"
        android:name="com.example.itmolab2.home.HomeOpenedFragment"
        android:label="homeOpenedFragment">
        <argument
            android:name="count"
            app:argType="integer" />
    </fragment>

</navigation>